{
  "name": "ecma-v",
  "version": "0.0.1",
  "description": "Checks the EcamScript version of .js glob against a specified version of EcamScript with a shell command",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "lint": "eslint index.js --fix",
    "report:coverage": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "see:coverage": "(COVERAGE=true ember test) && open ./coverage/index.html",
    "test": "nyc mocha test.js --timeout 10s --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dollarshaveclub/ecma-v.git"
  },
  "author": "Jeff Wainwright <yowainwright@gmail.com> (https://jeffry.in), Brian Gonzalez <me@briangonzalez.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dollarshaveclub/ecma-v/issues"
  },
  "homepage": "https://github.com/dollarshaveclub/ecma-v#readme",
  "peerDependencies": {
    "eslint": ">= 3"
  },
  "devDependencies": {
    "acorn": "^5.1.2",
    "assert": "^1.4.1",
    "codecov": "^2.3.0",
    "eslint": "^4.8.0",
    "eslint-plugin-dollarshaveclub": "^1.0.2",
    "glob": "^7.1.2",
    "globby": "^6.1.0",
    "mocha": "^4.0.1",
    "nyc": "^11.2.1",
    "pre-commit": "^1.2.2"
  },
  "dependencies": {
    "caporal": "^0.7.0"
  },
  "engines": {
    "node": ">= 7"
  },
  "keywords": [
    "check for es6",
    "check for modules",
    "es",
    "es check",
    "es matching",
    "es version",
    "ecmascript",
    "ecmascript check",
    "ecmascript matching",
    "ecmascript version",
    "js",
    "js version",
    "js version check",
    "js version matching",
    "test js version",
    "test ecmascript version"
  ]
}
